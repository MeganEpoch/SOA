<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
        targetNamespace="http://jw.nju.edu.cn/schema"
        xmlns:tns="http://jw.nju.edu.cn/schema"  elementFormDefault="qualified">

    <include schemaLocation="../Department.xsd"></include>

    <simpleType name="姓名类型">
        <restriction base="string">
            <minLength value="2"/>
        </restriction>
    </simpleType>
    
    <complexType name="个人信息类型">
        <sequence>
            <element name="姓名" type="tns:姓名类型"></element>
            <element name="所属部门" type="tns:部门编号类型"></element>
        </sequence>
    </complexType>
</schema>